{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}}âš¡{{end}}{{if .Segments.Git.RepoName}}{{.Segments.Git.RepoName}}{{else}}{{.Folder}}{{end}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "threshold": 999
          },
          "template": "<#333333>\ufbab</> {{ .FormattedMs }}\n",
          "foreground": "#666666",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "<#666666>\uf017</> 15:04"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#cccccc",
          "powerline_symbol": "\ue0b0",
          "background": "#333333",
          "type": "time",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "Â ",
          "type": "text"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_default": false
          },
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ if ne (upper .UserName) (upper .Env.USERNAME) }}{{ .UserName }}{{ end }}{{ if ne (upper .HostName) (upper .Env.COMPUTERNAME) }}@{{ .HostName }}{{ end }} ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#ffffff",
          "type": "session",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "Â ",
          "type": "text"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf115",
            "folder_separator_icon": " <#000000>\ue0b1</> ",
            "home_icon": "\uf7db",
            "style": "full"
          },
          "template": " {{ .Path }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#8800dd",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "Â ",
          "type": "text"
        },
        {
          "properties": {
            "branch_gone_icon": "ðŸ’€",
            "branch_icon": "\ue0a0 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_worktree_count": true
          },
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#95f8ff",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9900{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ffff99{{ end }}",
            "{{ if gt .Ahead 0 }}#ff9999{{ end }}",
            "{{ if gt .Behind 0 }}#99ff99{{ end }}"
          ],
          "templates": [
            " {{ .HEAD }}",
            " {{ .BranchStatus }}",
            " <#000000>\ue0b1</>",
            " {{ .Env.HEAD_REV }}",
            "{{ if .Working.Changed }} <#000000>\ue0b1</> \uf044 {{ .Working.String }}{{ end }}",
            "{{ if .Staging.Changed }} <#000000>\ue0b1</> \uf046 {{ .Staging.String }}{{ end }}",
            "{{ if gt .StashCount 0 }} <#000000>\ue0b1</> \uf692 {{ .StashCount }}{{ end }}",
            "{{ if gt .WorktreeCount 0 }} <#000000>\ue0b1</> \uf1bb {{ .WorktreeCount }}{{ end }}",
            "\u2800"
          ]
        }
      ]
    },
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "âš¡"
          },
          "template": "âš¡",
          "foreground": "#ffc83d",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u25b6",
          "foreground": "#007acc",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
