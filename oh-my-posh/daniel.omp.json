{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#666666",
                    "properties": {
                        "threshold": 999
                    },
                    "style": "plain",
                    "template": "<#333333>\ufbab</> {{ .FormattedMs }}\n",
                    "type": "executiontime"
                },
                {
                    "background": "#333333",
                    "foreground": "#cccccc",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "time_format": "<#666666>\uf017</> 15:04"
                    },
                    "style": "powerline",
                    "template": " {{ .CurrentDate | date .Format }} ",
                    "type": "time"
                },
                {
                    "template": "  ",
                    "type": "text"
                },
                {
                    "background": "#ffffff",
                    "foreground": "#100e23",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "display_default": false
                    },
                    "style": "powerline",
                    "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }}@{{ .HostName }} ",
                    "type": "session"
                },
                {
                    "template": "  ",
                    "type": "text"
                },
                {
                    "background": "#8800dd",
                    "foreground": "#ffffff",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "folder_icon": "\uf115",
                        "folder_separator_icon": " <#000000>\ue0b1</> ",
                        "home_icon": "\uf7db",
                        "style": "full"
                    },
                    "style": "powerline",
                    "template": " {{ .Path }} ",
                    "type": "path"
                },
                {
                    "template": "  ",
                    "type": "text"
                },
                {
                    "background": "#99ff99",
                    "foreground": "#000000",
                    "powerline_symbol": "\ue0b0",
                    "style": "powerline",
                    "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
                    "type": "dotnet"
                },
                {
                    "template": "  ",
                    "type": "text"
                },
                {
                    "background": "#95f8ff",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#ff9900{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ffff99{{ end }}",
                        "{{ if gt .Ahead 0 }}#ff9999{{ end }}",
                        "{{ if gt .Behind 0 }}#99ff99{{ end }}"
                    ],
                    "foreground": "#193549",
                    "powerline_symbol": "\ue0b0",
                    "properties": {
                        "branch_gone_icon": "ðŸ’€",
                        "branch_icon": "\ue0a0 ",
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "fetch_worktree_count": true
                    },
                    "style": "powerline",
                    "template": " {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} <#000000>\ue0b1</> \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }}{{ end }}{{ if .Staging.Changed }} <#000000>\ue0b1</> \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}}<#000000>\ue0b1</> \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "type": "newline"
        },
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#ffc83d",
                    "properties": {
                        "root_icon": "\u26a1"
                    },
                    "style": "plain",
                    "template": "\uf0e7",
                    "type": "root"
                },
                {
                    "foreground": "#007acc",
                    "style": "plain",
                    "template": "\u25b6",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}
