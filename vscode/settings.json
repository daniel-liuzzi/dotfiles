{
    "cSpell.userWords": [
        "hotstring",
        "hotstrings",
        "Xunit",
    ],
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 3,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.fontFamily": "MesloLGM NF",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.inlineSuggest.enabled": true,
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.quickSuggestions": true,
    "editor.renderWhitespace": "all",
    "editor.suggest.insertMode": "replace",
    "editor.suggestSelection": "first",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*.js": "${capture}.spec.${extname}",
        "*.ts": "${capture}.spec.${extname}"
    },
    "files.hotExit": "off",
    "diffEditor.renderSideBySide": false,
    "javascript.suggest.completeFunctionCalls": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.autofetch": true,
    "groovyLint.java.options": "-Xms256m,-Xmx1024m",
    "omnisharp.enableDecompilationSupport": true,
    "powershell.integratedConsole.showOnStartup": false,
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.fontFamily": "MesloLGM NF",
    "terminal.integrated.defaultProfile.windows": "PowerShell Core",
    "terminal.integrated.profiles.windows": {
        "PowerShell Core": {
            "path": "pwsh",
            "args": [
                "-NoLogo"
            ]
        }
    },
    "typescript.suggest.completeFunctionCalls": true,
    "vsicons.dontShowNewVersionMessage": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.restoreWindows": "none",
    "window.title": "${rootName}${separator}${dirty}${separator}${activeEditorMedium}",
    "window.titleSeparator": " â€” ",
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#66ff66",
        "editor.lineHighlightBackground": "#303030",
        "editorLineNumber.foreground": "#444444",
        "editorLineNumber.activeForeground": "#aaaaaa",
        "editorWhitespace.foreground": "#333333",
        "terminal.ansiBlack": "#0f1610",
        "terminal.ansiBrightBlack": "#666666",
        "terminal.ansiWhite": "#999999",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiRed": "#993333",
        "terminal.ansiBrightRed": "#cc6666",
        "terminal.ansiGreen": "#336633",
        "terminal.ansiBrightGreen": "#669966",
        "terminal.ansiBlue": "#0066cc",
        "terminal.ansiBrightBlue": "#0099ff",
        "terminal.ansiYellow": "#666633",
        "terminal.ansiBrightYellow": "#999966",
        "terminal.ansiCyan": "#336666",
        "terminal.ansiBrightCyan": "#669999",
        "terminal.ansiMagenta": "#663366",
        "terminal.ansiBrightMagenta": "#996699",
        "terminal.ansiForeground": "#eff0eb",
        // "terminal.background": "#0f1610",
    },
    "workbench.colorTheme": "Nord",
    "workbench.editorAssociations": {
        "*.md": "vscode.markdown.preview.editor",
        "git-rebase-todo": "default",
    },
    "workbench.editor.restoreViewState": false,
    "workbench.editor.untitled.hint": "hidden",
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 18,
}