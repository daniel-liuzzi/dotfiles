{
    "[html]": {
        "editor.renameOnType": true,
    },
    "editor.cursorSmoothCaretAnimation": true,
    "editor.fontFamily": "Cascadia Code PL",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.mouseWheelZoom": true,
    "editor.renderWhitespace": "all",
    "editor.scrollbar.vertical": "visible", // https://github.com/microsoft/vscode/issues/35454
    "editor.scrollbar.horizontal": "visible", // https://github.com/microsoft/vscode/issues/35454
    "editor.suggest.insertMode": "replace",
    "editor.suggestSelection": "first",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "diffEditor.renderSideBySide": false,
    "javascript.suggest.completeFunctionCalls": true,
    "git.autofetch": true,
    "powershell.notebooks.showToggleButton": true,
    "terminal.integrated.fontFamily": "MesloLGM NF",
    "terminal.integrated.shell.windows": "C:/Program Files/PowerShell/7/pwsh.exe",
    "terminal.integrated.shellArgs.windows": [
        "-NoLogo"
    ],
    "typescript.suggest.completeFunctionCalls": true,
    "vsicons.dontShowNewVersionMessage": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorCustomizations": {
        "terminal.ansiBlack": "#0f1610",
        "terminal.ansiBrightBlack": "#666666",
        "terminal.ansiWhite": "#999999",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiRed": "#993333",
        "terminal.ansiBrightRed": "#cc6666",
        "terminal.ansiGreen": "#336633",
        "terminal.ansiBrightGreen": "#669966",
        "terminal.ansiBlue": "#0066cc",
        "terminal.ansiBrightBlue": "#0099ff",
        "terminal.ansiYellow": "#666633",
        "terminal.ansiBrightYellow": "#999966",
        "terminal.ansiCyan": "#336666",
        "terminal.ansiBrightCyan": "#669999",
        "terminal.ansiMagenta": "#663366",
        "terminal.ansiBrightMagenta": "#996699",
        "terminal.ansiForeground": "#eff0eb",
        // "terminal.background": "#0f1610",
    },
    "workbench.colorTheme": "GitHub Dark",
    "workbench.editorAssociations": [
        {
            "viewType": "default",
            "filenamePattern": "git-rebase-todo"
        }
    ],
    "workbench.editor.restoreViewState": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 18,
}
