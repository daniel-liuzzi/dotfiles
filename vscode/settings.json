{
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp",
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[powershell]": {
        "editor.defaultFormatter": "ms-vscode.powershell",
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "cSpell.userWords": [
        "hotstring",
        "hotstrings",
        "Liuzzi",
        "Xunit",
    ],
    "debug.onTaskErrors": "abort",
    "diffEditor.diffAlgorithm": "advanced",
    "editor.accessibilitySupport": "off",
    "editor.guides.bracketPairs": true,
    "editor.guides.bracketPairsHorizontal": true,
    "editor.cursorBlinking": "expand",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorWidth": 3,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.fontFamily": "ComicMono NF",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.inlineSuggest.enabled": true,
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
    },
    "editor.renderWhitespace": "all",
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.suggest.insertMode": "replace",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*.js": "${capture}.spec.${extname}",
        "*.ts": "${capture}.spec.${extname}"
    },
    "files.associations": {
        ".wslconfig": "ini",
        "**/git/config**": "ini",
    },
    "files.hotExit": "off",
    "diffEditor.renderSideBySide": false,
    "javascript.suggest.completeFunctionCalls": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.autofetch": true,
	"git.autofetchPeriod": 60,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": false,
        "scminput": false
    },
    "groovyLint.java.options": "-Xms256m,-Xmx1024m",
    "omnisharp.enableDecompilationSupport": true,
    "oracledevtools.bookmarkFileFolder": "${env:USERPROFILE}\\Oracle\\oracle.oracledevtools",
    "oracledevtools.connectionConfiguration.configFilesFolder": "${env:USERPROFILE}\\Oracle\\network\\admin",
    "oracledevtools.connectionConfiguration.walletFileFolder": "${env:USERPROFILE}\\Oracle\\network\\admin",
    "oracledevtools.download.otherFolder": "${env:USERPROFILE}\\downloads",
    "powershell.integratedConsole.showOnStartup": false,
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.defaultProfile.windows": "PowerShell Core",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.profiles.windows": {
        "PowerShell Core": {
            "path": "pwsh",
            "args": [
                "-NoLogo"
            ]
        }
    },
    "typescript.suggest.completeFunctionCalls": true,
    "window.commandCenter": true,
    "window.newWindowDimensions": "inherit",
    "window.restoreWindows": "none",
    "window.title": "${rootName}${separator}${dirty}${separator}${activeEditorMedium}",
    "window.titleSeparator": " â€” ",
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#66ff66",
        "editor.lineHighlightBackground": "#303030",
        "editorLineNumber.foreground": "#444444",
        "editorLineNumber.activeForeground": "#aaaaaa",
        "editorWhitespace.foreground": "#333333",
        "terminal.ansiBlack": "#0f1610",
        "terminal.ansiBrightBlack": "#666666",
        "terminal.ansiWhite": "#999999",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiRed": "#993333",
        "terminal.ansiBrightRed": "#cc6666",
        "terminal.ansiGreen": "#336633",
        "terminal.ansiBrightGreen": "#669966",
        "terminal.ansiBlue": "#0066cc",
        "terminal.ansiBrightBlue": "#0099ff",
        "terminal.ansiYellow": "#666633",
        "terminal.ansiBrightYellow": "#999966",
        "terminal.ansiCyan": "#336666",
        "terminal.ansiBrightCyan": "#669999",
        "terminal.ansiMagenta": "#663366",
        "terminal.ansiBrightMagenta": "#996699",
        "terminal.ansiForeground": "#eff0eb",
        // "terminal.background": "#0f1610",
    },
    "workbench.editorAssociations": {
        "git-rebase-todo": "default",
    },
    "workbench.editor.restoreViewState": false,
    "workbench.editor.untitled.hint": "hidden",
    "workbench.list.smoothScrolling": true,
    "workbench.settings.editor": "json",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 18,
    "xml.server.binary.trustedHashes": [
        "a1bf296a5263bc63cca97cb46692d0d523edfe9d2d4e79d5b4d245f57c6c623b",
    ],
}