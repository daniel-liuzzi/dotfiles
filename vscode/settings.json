{
    "[html]": {
        "editor.renameOnType": true,
    },
    "editor.cursorSmoothCaretAnimation": true,
    "editor.fontFamily": "Cascadia Code PL",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.renderWhitespace": "all",
    "editor.scrollbar.vertical": "visible", // https://github.com/microsoft/vscode/issues/35454
    "editor.scrollbar.horizontal": "visible", // https://github.com/microsoft/vscode/issues/35454
    "editor.suggestSelection": "first",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "diffEditor.renderSideBySide": false,
    "git.autofetch": true,
    "mssql.connections": [
        {
            "server": ".\\SQLEXPRESS",
            "database": "Crs",
            "authenticationType": "Integrated",
            "profileName": ".\\SQLEXPRESS - Crs",
            "password": "",
        },
    ],
    "powershell.notebooks.showToggleButton": true,
    "remote.SSH.remotePlatform": {
        "web7.lan.mlginc.com": "windows",
    },
    "terminal.integrated.shell.windows": "${env:ProgramFiles}/PowerShell/7/pwsh.exe",
    "terminal.integrated.shellArgs.windows": ["-NoLogo"],
    "todo-tree.regex.regex": "((//|#|<!--|;|/\\*|^|')\\s*($TAGS)|^\\s*- \\[ \\])",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorCustomizations": {
        "terminal.ansiBlack": "#0f1610",
        "terminal.ansiBrightBlack": "#666666",
        "terminal.ansiWhite": "#999999",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiRed": "#993333",
        "terminal.ansiBrightRed": "#cc6666",
        "terminal.ansiGreen": "#336633",
        "terminal.ansiBrightGreen": "#669966",
        "terminal.ansiBlue": "#0066cc",
        "terminal.ansiBrightBlue": "#0099ff",
        "terminal.ansiYellow": "#666633",
        "terminal.ansiBrightYellow": "#999966",
        "terminal.ansiCyan": "#336666",
        "terminal.ansiBrightCyan": "#669999",
        "terminal.ansiMagenta": "#663366",
        "terminal.ansiBrightMagenta": "#996699",
        "terminal.ansiForeground": "#eff0eb",
        // "terminal.background": "#0f1610",
    },
    "workbench.colorTheme": "SynthWave '84",
    "workbench.editor.enablePreview": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "none",
}
